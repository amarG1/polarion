				&lt;/td&gt;
				&lt;td&gt;
					#set($query = &quot;$versionQuery AND type:workpackage&quot;)
					&lt;span class=&quot;polarion-rp-inline-widget&quot; data-widget=&quot;com.polarion.countOfObjects&quot;&gt;
