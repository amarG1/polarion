			&lt;th&gt;$systemRequirement.render()s&lt;/th&gt;
			#set($workPackage = $transaction.enumerations().getEnumeration(&quot;workitem-type&quot;).forProject(&quot;$projectId&quot;).byId(&quot;workpackage&quot;))
